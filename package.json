{
  "name": "waelio-utils",
  "version": "3.2.12",
  "description": "Helper Utils often used in websites",
  "homepage": "https://github.com/waelio/waelio-utils#index",
  "keywords": [
    "utilities",
    "browser",
    "quasar",
    "vue",
    "javascript",
    "helpers",
    "react",
    "angular",
    "nuxt3"
  ],
  "files": [
    "dist/**",
    "src/*"
  ],
  "directories": {
    "src": "src",
    "dist": "dist",
    "test": "spec"
  },
  "sideEffects": false,
  "main": "dist/waelioUtils",
  "size-limit": [
    {
      "path": "dist/waelioUtils"
    }
  ],
  "types": "dist/types",
  "scripts": {
    "clean": "node ./tools/clean.ts",
    "size": "rollup -c && size-limit",
    "start": "rollup -c",
    "test": "jasmine && size-limit"
  },
  "exports": {
    ".": "./dist/waelioUtils.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "waelioUtils": [
        "dist/waelioUtils.d.ts"
      ]
    }
  },
  "author": {
    "name": "Peace Marshal",
    "email": "wahbehw@me.com",
    "url": "https://waelio.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waelio/waelio-utils.git"
  },
  "bugs": {
    "url": "https://github.com/waelio/waelio-utils/issues"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.4.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "axios": "^0.21.1",
    "eslint": "^7.15.0",
    "jasmine": "^3.7.0",
    "magic-string": "^0.25.7",
    "rollup": "^2.43.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-ts-treeshaking": "^1.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  }
}
